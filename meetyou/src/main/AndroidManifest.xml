<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="com.example.annawang.meetyou">

    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"/>
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE"/>

    <application
        android:name="com.example.annawang.meetyou.MeetYouApp_"
        android:allowBackup="true"
        android:icon="@mipmap/ic_brightness_3_lime_100_36dp"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_brightness_3_lime_100_36dp"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppCompat.Light.NoActionBar">
        <activity android:name="com.example.annawang.meetyou.ui.MeetActivity_">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>

    </application>
</manifest>